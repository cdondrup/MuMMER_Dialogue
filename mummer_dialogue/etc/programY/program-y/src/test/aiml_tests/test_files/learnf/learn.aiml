<aiml>

	<category>
		<pattern>MY NAME IS *</pattern>
		<template>
			<think>
				<set name="myname"><star /></set>
			</think>
		    <learn>
				<category>
					<pattern>WHAT IS MY NAME</pattern>
					<template>YOUR NAME IS <eval><get name="myname" /></eval></template>
				</category>
			</learn>
			OK, I will remember your name is <get name="myname" />
		</template>
	</category>

</aiml>